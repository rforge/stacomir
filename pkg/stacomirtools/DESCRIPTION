    'RequeteODBC.r'
    'RequeteODBCwhere.r'
    'RequeteODBCwheredate.r'
    'utilities.r'   
    'stacomirtools.r'
    'stacomirtools-package.R' 
LazyLoad: yes
Depends: RODBC
